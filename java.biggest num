import java.util.Scanner;
class Main
{  
public static void main(String[] args)  
{    
Scanner sc=new Scanner(System.in);    
int n;    
System.out.println("enter size");    
n=sc.nextInt();    
int[] a=new int[n];    
System.out.println("enter elements");    
for(int i=0;i<n;i++)    
{ 
a[i]=sc.nextInt();    
}    
int large=a[0];    
for(int i=0;i<n;i++)    
{      
if(large<a[i])      
{        
large=a[i];      
}    
}    
System.out.println("biggest number is"+large);  
}
}

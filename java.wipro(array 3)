//write a program to initialize an integer array with values print -1 if given number present ina n array otherwise print index of a given number
import java.util.Scanner;
class arraythree
{
	public static void main(String[] args)
	{
		int[] arr={1,4,34,56,7};
		int n=sc.nextInt();
		int index=0;
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]==n)
			{
				index+=i;
			}
		}
		if(index==0)
		{
			System.out.println(-1);
		}
		else
		{
			System.out.println(index);
		}
	}
}

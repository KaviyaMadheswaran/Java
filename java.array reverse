import java.util.*;
class Main
{
  public static void main(String[] args)
  {
    Scanner sc=new Scanner(System.in);
    int[] arr={1,2,3,4};
    int n=arr.length;
    int[] ind=new int[n];
    for(int i=n;i>0;i--)
    {
      ind[n-i]=arr[i-1];
    }
    System.out.println(Arrays.toString(ind));
  }
}

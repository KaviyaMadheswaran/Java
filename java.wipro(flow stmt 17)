//write a program to check a given number is palindrome or not
import java.util.Scanner;
class flowseventeen
{
public static void main(String[] args)
{
	int n=args[0];
	int n1=n;
	int rev=0;
	while(n!=0)
	{
		int a=n%10;
		rev=rev*10+a;
		n/=10;
	}
	if(n1==rev)
	{
		System.out.println(n1+" "+"is"+" "+"a"+" "+"palindrome");
	}
	else
	{
		System.out.println(n1+" "+"is"+" "+" "+"not"+" "+"a"+" "+"palindrome");
	}
}
}

//check whether the program has received command line arguments or not
class flowthree
{
public static void main(String args[])
{
String ans="";
if(args.length==0)
{
System.out.println("No values");
}
else
{
for(int i=0;i<args.length-1;i++)
{
ans+=args[i]+",";
}
System.out.print(ans+=args[args.length-1]);
}
}
}

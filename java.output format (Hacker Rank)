import java.util.Scanner;
class Main
{ 
    public static void main(String[] args) 
    { 
        Scanner sc=new Scanner(System.in); 
        System.out.println("======================"); 
        for(int i=0;i<3;i++) 
        { 
            String s1=sc.next(); 
            int x=sc.nextInt(); 
            for(int j=0;j<15;j++) 
            { 
                if(s1.length()<15) 
                { 
                    s1=s1+" "; 
                } 
            } 
            System.out.print(s1); 
            System.out.println(String.format("%03d",x));
        } 
        System.out.println("=========================");
    }
    
}

import java.util.Scanner;
class Main
{  
public void add(int a,int b)  
{    
int c=a+b;    
System.out.println(c);  
}  
public int sub(int a,int b)  
{    
int d=a-b;    
return d;  
}    
public static void main(String[] args)    
{      
Scanner sc=new Scanner(System.in);      
int a=sc.nextInt();      
int b=sc.nextInt();      
Main ob=new Main();      
ob.add(a,b);      
System.out.println(ob.sub(a,b));    
}  
}

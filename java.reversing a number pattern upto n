import java.util.Scanner;
class Main 
{ 
  public static void main(String[] args) 
  { 
    Scanner sc=new Scanner(System.in); 
    int n,i,j=1; 
    System.out.println("range"); 
    n=sc.nextInt(); 
    for(i=n;i>=1;i--) 
    { 
      for(j=1;j<=i;j++) 
      { 
        System.out.print(j+"\t"); 
      } 
        System.out.println("\n");
     } 
  }
}

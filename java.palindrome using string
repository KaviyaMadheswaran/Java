import java.util.Scanner;
class Main 
{ 
  public static void main(String[] args) 
  { 
    Scanner sc=new Scanner(System.in); 
    String s=sc.nextLine();
    int a=s.length();
    String r="";
    for(int i=a-1;i>=0;i--)
    {
      r=r+s.charAt(i);
    }
    if(r.equals(s))
    {
      System.out.println("palindrome");
    }
    else
    {
      System.out.println("not palindrome");
    }
  } 
}
